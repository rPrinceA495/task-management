  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon server/index.js --watch server common --exec babel-node",
    "start:prod": "cross-env NODE_ENV=production node dist/server",
    "clean": "rimraf dist",
    "copy:server": "copyfiles server/**/*.json server/**/*.html dist",
    "build:client": "cross-env NODE_ENV=production webpack -p --config client/webpack.config.babel.js --progress",
    "build:common": "cross-env NODE_ENV=production babel common -d dist/common",
    "build:server": "cross-env NODE_ENV=production babel server -d dist/server && npm run copy:server",
    "build": "npm run clean && npm run build:client && npm run build:common && npm run build:server",
    "test:api": "jest --config test/api/jest.json",
    "test": "npm run test:api",
    "lint": "eslint client server common --ext .js,.jsx",
    "create-tables": "cross-env NODE_ENV=development babel-node server/createTables.js",
    "recreate-tables": "cross-env NODE_ENV=development babel-node server/createTables.js --force"
  }
}
